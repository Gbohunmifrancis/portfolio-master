{
  "name": "portfolio-blog-fullstack",
  "version": "1.0.0",
  "description": "Portfolio blog with Vue frontend and Express/MongoDB backend",
  "scripts": {
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "vite --host",
    "server": "cd server && npm run dev",
    "build": "vite build",
    "build:server": "cd server && npm start",
    "install:all": "npm install && cd server && npm install",
    "setup": "npm run install:all"
  },
  "dependencies": {
    "@kalimahapps/vue-icons": "^1.4.0",
    "@tiptap/extension-color": "^2.1.11",
    "@tiptap/extension-text-align": "^2.1.11", 
    "@tiptap/extension-text-style": "^2.1.11",
    "@tiptap/starter-kit": "^2.1.11",
    "@tiptap/vue-3": "^2.1.11",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^9.0.0",
    "express": "^4.18.2",
    "gsap": "^3.12.5",
    "marked": "^9.0.0",
    "mongoose": "^7.5.0",
    "onchange": "^7.1.0",
    "pinia": "^2.1.7",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "sass": "^1.74.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-use-active-scroll": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.4",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.11"
  }
}