{
	"name": "custom-portfolio",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite --host",
		"dev:fullstack": "concurrently \"npm run dev\" \"npm run server:dev\"",
		"server:dev": "cd server && npm run dev",
		"server:start": "cd server && npm start",
		"build": "run-p type-check build-only",
		"preview": "vite preview",
		"build-only": "vite build",
		"prettier": "onchange \"src/**/*\" -- prettier --write {{changed}}",
		"type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
	},
	"dependencies": {
		"@kalimahapps/vue-icons": "^1.4.0",
		"@tiptap/extension-color": "^3.6.1",
		"@tiptap/extension-font-family": "^3.6.1",
		"@tiptap/extension-text-align": "^3.6.1",
		"@tiptap/extension-text-style": "^3.6.1",
		"@tiptap/starter-kit": "^3.6.1",
		"@tiptap/vue-3": "^3.6.1",
		"@vueuse/components": "^10.9.0",
		"@vueuse/core": "^10.9.0",
		"axios": "^1.12.2",
		"concurrently": "^9.2.1",
		"cors": "^2.8.5",
		"dotenv": "^17.2.2",
		"eslint": "^9.0.0",
		"express": "^5.1.0",
		"gsap": "^3.12.5",
		"marked": "^16.3.0",
		"mongoose": "^8.18.2",
		"nodemon": "^3.1.10",
		"onchange": "^7.1.0",
		"pinia": "^2.1.7",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"sass": "^1.74.1",
		"vue": "^3.4.21",
		"vue-router": "^4.3.0",
		"vue-use-active-scroll": "^1.1.1"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.4",
		"@types/node": "^20.12.5",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/tsconfig": "^0.5.1",
		"autoprefixer": "^10.4.19",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.3",
		"typescript": "~5.4.4",
		"vite": "^5.2.8",
		"vue-tsc": "^2.0.11"
	}
}
